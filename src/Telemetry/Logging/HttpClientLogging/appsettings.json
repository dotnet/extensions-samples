{
  "HttpClientLogging": {
    // This parameter controls whether it logs start of request processing:
    "LogRequestStart": false,

    "RequestPathParameterRedactionMode": "Strict",
    "RequestPathLoggingMode": "Structured",

    // This option should be turned off in production environments:
    "LogBody": false,
    "BodySizeLimit": 128,
    "BodyReadTimeout": "00:00:10",
    "RequestBodyContentTypes": [ ],
    "ResponseBodyContentTypes": [ "application/json", "text/*" ]
  }
}
