<Project>
  <PropertyGroup Label="Version settings">
    <MajorVersion>8</MajorVersion>
    <MinorVersion>0</MinorVersion>
    <PatchVersion>0</PatchVersion>
    <!-- version in our package name #.#.#-below.#####.## -->
    <PreReleaseVersionLabel>rtm</PreReleaseVersionLabel>
    <PreReleaseVersionIteration>
    </PreReleaseVersionIteration>
    <VersionPrefix>$(MajorVersion).$(MinorVersion).$(PatchVersion)</VersionPrefix>
    <AssemblyVersion>$(MajorVersion).$(MinorVersion).0.0</AssemblyVersion>
    <ValidateBaseline>true</ValidateBaseline>
    <!--
        When StabilizePackageVersion is set to 'true', this branch will produce stable outputs for 'Shipping' packages
    -->
    <StabilizePackageVersion Condition="'$(StabilizePackageVersion)' == ''">false</StabilizePackageVersion>
  </PropertyGroup>
  <!--

    These versions should ONLY be updated by automation.

    DO NOT UPDATE THESE MANUALLY. Use the `darc` command line tool to update this file so it stays in sync with
    Version.Details.xml.

    See https://github.com/dotnet/arcade/blob/master/Documentation/Darc.md for instructions on using darc.

  -->
  <PropertyGroup Label="Automated">
    <!-- dotnet/runtime -->
    <MicrosoftBclTimeProviderVersion>8.0.0-rtm.23520.16</MicrosoftBclTimeProviderVersion>
    <MicrosoftExtensionsHostingVersion>8.0.0-rtm.23520.16</MicrosoftExtensionsHostingVersion>
    <MicrosoftNETCoreAppRuntimewinx64Version>8.0.0-rtm.23520.16</MicrosoftNETCoreAppRuntimewinx64Version>
    <!-- dotnet/aspnetcore -->
    <MicrosoftAspNetCoreAppRuntimewinx64Version>8.0.0-rtm.23520.10</MicrosoftAspNetCoreAppRuntimewinx64Version>
    <!-- dotnet/extensions -->
    <MicrosoftAspNetCoreDiagnosticsMiddlewareVersion>8.0.0-rtm.23524.3</MicrosoftAspNetCoreDiagnosticsMiddlewareVersion>
    <MicrosoftExtensionsAmbientMetadataApplicationVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsAmbientMetadataApplicationVersion>
    <MicrosoftExtensionsComplianceRedactionVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsComplianceRedactionVersion>
    <MicrosoftExtensionsComplianceTestingVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsComplianceTestingVersion>
    <MicrosoftExtensionsDiagnosticsExceptionSummarizationVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsDiagnosticsExceptionSummarizationVersion>
    <MicrosoftExtensionsDiagnosticsExtraAbstractionsVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsDiagnosticsExtraAbstractionsVersion>
    <MicrosoftExtensionsDiagnosticsExtraVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsDiagnosticsExtraVersion>
    <MicrosoftExtensionsDiagnosticsTestingVersion>8.0.0-rtm.23524.3</MicrosoftExtensionsDiagnosticsTestingVersion>
  </PropertyGroup>
  <!--

    ^^^^^^^^^^
    SEE NOTE ABOVE.

    Versions above this comment are updated automatically. Don't change them manually.

    Versions below this comment are not managed by automation and can be changed as needed.
  -->
  <PropertyGroup Label="Manual">
    <MicrosoftNETTestSdkVersion>17.5.0</MicrosoftNETTestSdkVersion>
    <!-- XUnit-related (not extensions) -->
    <XUnitVersion>2.4.2</XUnitVersion>
    <XUnitAssertVersion>$(XUnitVersion)</XUnitAssertVersion>
    <XUnitRunnerConsoleVersion>$(XUnitVersion)</XUnitRunnerConsoleVersion>
    <XUnitRunnerVisualStudioVersion>2.4.3</XUnitRunnerVisualStudioVersion>
    <XUnitExtensibilityExecutionVersion>$(XUnitVersion)</XUnitExtensibilityExecutionVersion>
    <XUnitStaFactPackageVersion>1.0.33-beta</XUnitStaFactPackageVersion>
    <!-- Additional unchanging dependencies -->
    <NewtonsoftJsonVersion>13.0.1</NewtonsoftJsonVersion>
    <OpenTelemetryExporterConsoleVersion>1.4.0</OpenTelemetryExporterConsoleVersion>
    <OpenTelemetryExtensionsHostingVersion>1.4.0</OpenTelemetryExtensionsHostingVersion>
  </PropertyGroup>
</Project>
